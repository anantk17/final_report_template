@inbook{Sadighian2014,
  author        = {Sadighian, Alireza and Fernandez, Jos{\'e} M. and Lemay, Antoine and Zargar, Saman T.},
  editor        = {Danger, Jean Luc and Debbabi, Mourad and Marion, Jean-Yves and Garcia-Alfaro, Joaquin and Zincir Heywood, Nur},
  pages         = {161--177},
  publisher     = {Springer International Publishing},
  title         = {ONTIDS: A Highly Flexible Context-Aware and Ontology-Based Alert Correlation Framework},
  year          = {2014},
  address       = {Cham},
  isbn          = {978-3-319-05302-8},
  abstract      = {Several alert correlation approaches have been proposed to
		  date to reduce the number of non-relevant alerts and false
		  positives typically generated by Intrusion Detection
		  Systems (IDS). Inspired by the mental process of the
		  contextualisation used by security analysts to weed out
		  less relevant alerts, some of these approaches have tried
		  to incorporate contextual information such as: type of
		  systems, applications, users, and networks into the
		  correlation process. However, these approaches are not
		  flexible as they only perform correlation based on the
		  narrowly defined contexts. information resources available
		  to the security analysts while preserving the maximum
		  flexibility and the power of abstraction in both the
		  definition and the usage of such concepts, we propose
		  ONTIDS, a context-aware and ontology-based alert
		  correlation framework that uses ontologies to represent and
		  store the alerts information, alerts context, vulnerability
		  information, and the attack scenarios. ONTIDS employs
		  simple ontology logic rules written in Semantic
		  Query-enhance Web Rule Language (SQWRL) to correlate and
		  filter out non-relevant alerts. We illustrate the potential
		  usefulness and the flexibility of ONTIDS by employing its
		  reference implementation on two separate case studies,
		  inspired from the DARPA 2000 and UNB ISCX IDS evaluation
		  datasets.},
  bdsk-url-1    = {https://doi.org/10.1007/978-3-319-05302-8_10},
  bdsk-url-2    = {http://dx.doi.org/10.1007/978-3-319-05302-8_10},
  booktitle     = {Foundations and Practice of Security: 6th International Symposium, FPS 2013, La Rochelle, France, October 21-22, 2013, Revised Selected Papers},
  date-added    = {2017-10-22 18:53:36 +0000},
  date-modified = {2017-10-22 18:53:36 +0000},
  doi           = {10.1007/978-3-319-05302-8_10},
  keywords      = {intrusion detection},
  url           = {https://doi.org/10.1007/978-3-319-05302-8_10}
}

