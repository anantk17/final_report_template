@inbook{Nguyen2014,
  author        = {Nguyen, Dang and Park, Jaehong and Sandhu, Ravi},
  editor        = {Au, Man Ho and Carminati, Barbara and Kuo, C.-C. Jay},
  pages         = {15--27},
  publisher     = {Springer International Publishing},
  title         = {{Adopting Provenance-Based Access Control in OpenStack Cloud IaaS}},
  year          = {2014},
  address       = {Cham},
  isbn          = {978-3-319-11698-3},
  abstract      = {Provenance-based Access Control (PBAC) has recently risen
		  as an effective access control approach that can utilize
		  readily provided history information of underlying systems
		  to enhance various aspects of access control in a computing
		  environment. The adoption of PBAC capabilities to the
		  authorization engine of a multi-tenant cloud
		  Infrastructure-as-a-Service (IaaS) such as OpenStack can
		  enhance the access control capabilities of cloud systems.
		  Toward this purpose, we introduce tenant-awareness to the
		  PBAC C [14] model by capturing tenant as contextual
		  information in the attribute provenance data. Built on this
		  model, we present a cloud service architecture that
		  provides PBAC authorization service and management. We
		  discuss in depth the variations of PBAC authorization
		  deployment architecture within the OpenStack platform and
		  implement a proof-of-concept prototype. We analyze the
		  initial experimental results and discuss approaches for
		  potential improvements.},
  bdsk-url-1    = {http://dx.doi.org/10.1007/978-3-319-11698-3_2},
  booktitle     = {Network and System Security: 8th International Conference, NSS 2014, Xi'an, China, October 15-17, 2014, Proceedings},
  date-added    = {2017-07-12 16:37:05 +0000},
  date-modified = {2017-07-12 16:37:05 +0000},
  doi           = {10.1007/978-3-319-11698-3_2},
